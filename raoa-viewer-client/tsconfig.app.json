{
  "extends": "./tsconfig.json",
  "compilerOptions": {
    "outDir": "./out-tsc/app",
    "types": [
      "gapi",
      "gapi.auth2"
    ]
  },
  "files": [
    "src/main.ts",
    "src/polyfills.ts",
    "node_modules/@types/wicg-file-system-access/index.d.ts",
  ],
  "include": [
    "src/**/*.ts",
    "src/**/*.d.ts"
  ],
  "exclude": [
    "src/**/*.spec.ts"
  ]
}
